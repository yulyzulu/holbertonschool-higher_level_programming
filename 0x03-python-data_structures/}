int is_palindrome(listint_t **head)
{
	listint_t *temp1, *temp2;
	int i, j;

	temp1 = *head;
	temp2 = *head;
	i = 0;
	while (*head)
	{
		(*head)->next;
		i++;
	}
	i = i/2;
	for (j = 0; temp1 < i; j++)
